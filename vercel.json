{
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 3008,
      "maxBodySize": "15mb"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["POST", "GET"],
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ]
}
