{
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 3008,
      "maxBodyLength": 8388608,
      "bodyParser": {
        "sizeLimit": "15mb"
      }
    }
  },
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload",
      "methods": ["POST"],
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ]
}
